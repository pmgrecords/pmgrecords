.music(ng-controller='MusicController as music')
    .row
        .col-xl-3.col-lg-4.col-md-6.col-sm-6(ng-repeat='artist in music.artists')
            .artist-tile
                a(href='{{ artist.soundCloudUrl }}')
                    h3 {{ artist.name }}
                iframe(scrolling="no", frameborder="no", ng-src='{{ music.soundCloudUrl(artist) | trusted }}')

