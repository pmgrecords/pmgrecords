{
  "paths": {
    "dist": {
      "all": "dist/**/*",
      "html": "dist/",
      "css": "dist/css/",
      "js": "dist/js/",
      "assets": "dist/assets",
      "bower": "dist/bower_components/",
      "partials": "dist/partials/",
      "index": "dist/",
      "fonts": "dist/fonts/"
    },
    "src": {
      "jade": "src/templates/**/*.jade",
      "sass": "src/css/**/*.scss",
      "assets": "src/assets/**",
      "bower": "bower_components/",
      "js": "src/js/**/*.js",
      "partials": "src/partials/**/*.html",
      "index": "src/index.html",
      "fonts": "src/fonts/**/*"
    }
  }
}